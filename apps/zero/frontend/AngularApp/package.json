{
  "name": "angular-app-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "startPowershell": "npx ng serve --ssl=true --ssl-key=$env:WML_CERT_KEY0 --ssl-cert=$env:WML_CERT0",
    "build": "npm run lint;ng build -c azureSWAProd",
    "deploy": "npx swa deploy ./dist/azureSWA",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --karma-conf=karma.conf.circle-ci.js",
    "lint": "ng lint --fix",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/angular-app-v2/stats.json"
  },
  "private": true,
