{
  "$schema": "https://typedoc.org/schema.json",
  "out": "docs",
  "media": "./assets",
  "plugin": ["typedoc-plugin-markdown", "@moralisweb3/typedoc-readme-theme"],
  "theme": "readme",
  "entryDocument": "nodejs-sdk-references.md",
  "entryPoints": ["packages/*"],
  "exclude": ["packages/eslintConfig"],
  "entryPointStrategy": "packages",
  "logLevel": "Verbose",
  "intentionallyNotExported": [],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "readme": "none",
  "excludeNotDocumented": true,
  "exclude": ["packages/eslintConfig"],
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  },
  "disableSources": true,
  "hideGenerator": true,
  "searchInComments": true,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": false,
    "external": false,
    "@alpha": false,
    "@beta": false
  },
  "sort": ["source-order"]
}
